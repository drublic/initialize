{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "import/first": "off",
    "import/extensions": "off",
    "max-len": ["error", 140]
  }
}
